<div class="row">
	<div class="col-xs-12">
		<!-- PAGE CONTENT BEGINS -->
			<% if (oneModel){ %>
			<form class="form-horizontal" id="js-updateForm" data-value="<%= oneModel%>" method="POST" action="/updateData" role="form">
				<input type="hidden" id="time" name="oneModel[date]" vulue="" />
				<div class="clearfix form-actions">
					<div class="col-md-offset-3 col-md-9">
						<button class="btn btn-info" type="submit">
							<i class="icon-ok bigger-110"></i>
							提交
						</button>

						&nbsp; &nbsp; &nbsp;
						<button class="btn" type="reset">
							<i class="icon-undo bigger-110"></i>
							重置
						</button>
					</div>
				</div>
			</form>
			<% } else { %>
			<form class="form-horizontal" id="js-addForm" data-value="_list" method="POST" action="/addData" role="form">
				<input type="hidden" id="time" name="_list[date]" vulue="" />
				<div class="clearfix form-actions">
					<div class="col-md-offset-3 col-md-9">
						<button class="btn btn-info" type="submit">
							<i class="icon-ok bigger-110"></i>
							提交
						</button>

						&nbsp; &nbsp; &nbsp;
						<button class="btn" type="reset">
							<i class="icon-undo bigger-110"></i>
							重置
						</button>
					</div>
				</div>
			</form>
			<% }%>
	</div><!-- /.col -->
</div><!-- /.row -->
<script src="/javascripts/adddata.js"></script>
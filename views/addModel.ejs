<div class="row">
	<div class="col-xs-12">
		<!-- PAGE CONTENT BEGINS -->
		<% if (oneModel){ %>
			<form class="form-horizontal" method="POST" action="/updateModel" role="form">
				<input type="hidden", name="oneModel[_id]", value="<%= oneModel._id%>" />
				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-1"> 名称 </label>

					<div class="col-sm-9">
						<input type="text" name="oneModel[name]" value="<%= oneModel.name%>" id="form-field-1" placeholder="名称" class="col-xs-10 col-sm-5" />
					</div>
				</div>

				<div class="space-4"></div>

				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 类型 </label>

					<div class="col-sm-9">
						<input type="text" name="oneModel[type]" value="<%= oneModel.type%>" id="form-field-2" placeholder="类型" class="col-xs-10 col-sm-5" />
						
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 发布人 </label>

					<div class="col-sm-9">
						<input type="text" name="oneModel[author]" value="<%= oneModel.author%>" id="form-field-2" placeholder="发布人" class="col-xs-10 col-sm-5" />
						
					</div>
				</div>
				<div class="space-4"></div>
				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 结构 </label>

					<div class="col-sm-9">
						<textarea id="form-field-11" name="oneModel[struts]" value="<%= oneModel.struts%>" class="autosize-transition col-xs-10 col-sm-5" style="overflow: hidden; word-wrap: break-word; resize: horizontal; height: 132px;"><%= oneModel.struts%></textarea>
						
					</div>
					
				</div>
				<input type="hidden" id="time" name="oneModel[date]" vulue="<%= oneModel.date%>" />
				<div class="clearfix form-actions">
					<div class="col-md-offset-3 col-md-9">
						<button class="btn btn-info" type="submit">
							<i class="icon-ok bigger-110"></i>
							修改
						</button>

						&nbsp; &nbsp; &nbsp;
						<button class="btn" type="reset">
							<i class="icon-undo bigger-110"></i>
							重置
						</button>
					</div>
				</div>
			</form>
		<% } else { %>
			<form class="form-horizontal" method="POST" action="/addModel" role="form">
				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-1"> 名称 </label>

					<div class="col-sm-9">
						<input type="text" name="modelList[name]" id="form-field-1" placeholder="名称" class="col-xs-10 col-sm-5" />
					</div>
				</div>

				<div class="space-4"></div>

				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 类型 </label>

					<div class="col-sm-9">
						<input type="text" name="modelList[type]" id="form-field-2" placeholder="类型" class="col-xs-10 col-sm-5" />
						
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 发布人 </label>

					<div class="col-sm-9">
						<input type="text" name="modelList[author]" id="form-field-2" placeholder="发布人" class="col-xs-10 col-sm-5" />
						
					</div>
				</div>
				<div class="space-4"></div>
				<div class="form-group">
					<label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 结构 </label>

					<div class="col-sm-9">
						<textarea id="form-field-11" name="modelList[struts]" class="autosize-transition col-xs-10 col-sm-5" style="overflow: hidden; word-wrap: break-word; resize: horizontal; height: 132px;"></textarea>
						
					</div>
					
				</div>
				<input type="hidden" id="time" name="modelList[date]" vulue="" />
				<div class="clearfix form-actions">
					<div class="col-md-offset-3 col-md-9">
						<button class="btn btn-info" type="submit">
							<i class="icon-ok bigger-110"></i>
							提交
						</button>

						&nbsp; &nbsp; &nbsp;
						<button class="btn" type="reset">
							<i class="icon-undo bigger-110"></i>
							重置
						</button>
					</div>
				</div>
			</form>
		<% }%>
		
	</div><!-- /.col -->
</div><!-- /.row -->
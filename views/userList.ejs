<div class="col-xs-12">
	<div class="table-responsive" id='js-data-table'>
		<table id="sample-table-2" class="table table-striped table-bordered table-hover">
			<thead>
				<tr>
					<th class="center">
						<label>
							<input type="checkbox" class="ace" />
							<span class="lbl"></span>
						</label>
					</th>
					<th>名字</th>
					<th>权限</th>
					<th>更新时间</th>
					<th>操作</th>
				</tr>
			</thead>

			<tbody>
				<% users.forEach(function(user,index){%>
				<tr id="js-type">
					<td class="center">
						<label>
							<input class="js-id" type="hidden" value="<%= user._id %>">
							<input type="checkbox" class="ace" />
							<span class="lbl"></span>
						</label>
					</td>
					<td>
						<%= user.name%>
					</td>
					<td>
						<%= user.role%>
					</td>
					<td><%= user.date%></td>
					<td>
						<div class="visible-md visible-lg hidden-sm hidden-xs action-buttons">

							<a class="green" href="updateUser/<%= user._id %>">
								<i class="icon-pencil bigger-130"></i>
							</a>

							<a class="red" href="deleteUser/<%= user._id %>">
								<i class="icon-trash bigger-130"></i>
							</a>
						</div>
					</td>
				</tr>
				<% })%>
			</tbody>
		</table>
	</div>

</div>
